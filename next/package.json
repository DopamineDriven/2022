{
  "name": "next",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "graphql:let": "graphql-let --config .graphql-let.yml -r dotenv/config",
    "graphql:codegen": "graphql-codegen --config codegen.yml -r dotenv/config",
    "generate:secret": "openssl rand -hex 32",
    "dev": "next dev -p ${PORT:=3040}",
    "dev:debug": "NODE_OPTIONS='--inspect' next -p ${PORT:=3040}",
    "build": "yarn graphql:let && next build",
    "start": "next start -p ${PORT:=3040}",
    "prod:build": "yarn graphql:let && yarn build",
    "docker:build:webapp": "docker build -t nextjs-docker .",
    "docker:run:webapp": "docker run -p 3040:3040 nextjs-docker",
    "analyze": "cross-env ANALYZE=true next build",
    "analyze:size": "cross-env ANALYZE=1 next build && cat .next/stats.txt",
    "find:unused": "next-unused",
    "tsconfig:effective": "tsc --showConfig",
    "pretty:quick": "yarn pretty-quick --staged",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "format": "prettier --write .",
    "type-check": "tsc --pretty --noEmit"
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "eslintIgnore": [
    "/pgdata/*"
  ],
  "next-unused": {
    "alias": {
      "@/apollo/*": [
        "src/apollo/*"
      ],
      "@/cache/*": [
        "src/.cache/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/graphql/*": [
        "src/graphql/*"
      ],
      "@/lib/*": [
        "src/lib/*"
      ],
      "@/pages/*": [
        "src/pages/*"
      ],
      "@/styles/*": [
        "src/styles/*"
      ],
      "@/types/*": [
        "src/types/*"
      ],
      "@/utils/*": [
        "src/utils/*"
      ]
    },
    "debug": true,
    "include": [
      "src/apollo",
      "src/components",
      "src/gql",
      "src/graphql",
      "src/lib",
      "src/pages",
      "src/server",
      "src/styles",
      "src/types",
      "src/utils"
    ],
    "exclude": [
      "node_modules"
    ],
    "entrypoints": [
      "src/pages"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.5.8",
    "@graphql-tools/graphql-file-loader": "^7.3.3",
    "@graphql-tools/load": "^7.5.1",
    "@graphql-tools/schema": "^8.3.1",
    "@graphql-tools/url-loader": "^7.7.1",
    "@headlessui/react": "^1.4.3",
    "@react-spring/web": "^9.4.2",
    "apollo-server-micro": "^3.6.2",
    "apollo-upload-client": "^17.0.0",
    "autoprefixer": "^10.4.2",
    "body-scroll-lock": "^4.0.0-beta.0",
    "classnames": "^2.3.1",
    "cookies-next": "^2.0.3",
    "date-fns": "^2.28.0",
    "debounce": "^1.2.1",
    "graphql": "^15.8.0",
    "graphql-let": "^0.18.6",
    "graphql-scalars": "^1.14.1",
    "graphql-upload": "^13.0.0",
    "html-react-parser": "^1.4.7",
    "keen-slider": "^6.6.3",
    "micro": "^9.3.4",
    "next": "^12.0.10",
    "next-rpc": "^3.4.0",
    "next-seo": "^5.1.0",
    "next-superjson": "^0.0.2",
    "next-themes": "^0.0.15",
    "postcss": "^8.4.6",
    "postcss-import": "^14.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-intersection-observer": "^8.33.1",
    "react-merge-refs": "^1.1.0",
    "react-use-measure": "^2.1.1",
    "superjson": "^1.8.0",
    "swr": "^1.2.1",
    "tabbable": "^5.2.1",
    "tailwindcss": "^3.0.18",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.0",
    "@graphql-codegen/cli": "^2.5.0",
    "@graphql-codegen/fragment-matcher": "^3.2.1",
    "@graphql-codegen/import-types-preset": "^2.1.10",
    "@graphql-codegen/introspection": "^2.1.1",
    "@graphql-codegen/plugin-helpers": "^2.4.0",
    "@graphql-codegen/schema-ast": "^2.4.1",
    "@graphql-codegen/typed-document-node": "^2.2.3",
    "@graphql-codegen/typescript": "^2.4.3",
    "@graphql-codegen/typescript-apollo-client-helpers": "^2.1.10",
    "@graphql-codegen/typescript-document-nodes": "^2.2.3",
    "@graphql-codegen/typescript-operations": "^2.2.4",
    "@graphql-codegen/typescript-react-apollo": "^3.2.5",
    "@graphql-codegen/typescript-resolvers": "^2.5.0",
    "@next/bundle-analyzer": "^12.0.10",
    "@tailwindcss/aspect-ratio": "^0.4.0",
    "@tailwindcss/forms": "^0.4.0",
    "@tailwindcss/typography": "^0.5.1",
    "@types/apollo-upload-client": "^17.0.0",
    "@types/body-scroll-lock": "^3.1.0",
    "@types/debounce": "^1.2.1",
    "@types/graphql-upload": "^8.0.10",
    "@types/gtag.js": "^0.0.8",
    "@types/jest": "^27.4.0",
    "@types/lodash.debounce": "^4.0.6",
    "@types/lodash.isequal": "^4.5.5",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.random": "^3.2.6",
    "@types/lodash.throttle": "^4.1.6",
    "@types/node": "^17.0.15",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@types/tailwindcss": "^3.0.7",
    "@types/uuid": "^8.3.4",
    "dotenv": "^16.0.0",
    "dotenv-cli": "^4.1.1",
    "eslint": "^8.8.0",
    "eslint-config-next": "^12.0.10",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-yml": "^0.13.0",
    "graphql-tag": "^2.12.6",
    "husky": "^7.0.4",
    "jest": "^27.5.0",
    "jest-createspyobj": "^2.0.0",
    "lint-staged": "^12.3.3",
    "next-unused": "^0.0.6",
    "patch-package": "^6.4.7",
    "postinstall-postinstall": "^2.1.0",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "stylelint": "^14.3.0",
    "stylelint-config-recommended": "^6.0.0",
    "ts-jest": "^27.1.3",
    "ts-toolbelt": "^9.6.0",
    "typescript": "^4.5.5",
    "utility-types": "^3.10.0",
    "webpack": "^5.68.0",
    "webpack-bundle-size-analyzer": "^3.1.0",
    "yaml-loader": "^0.6.0"
  }
}
