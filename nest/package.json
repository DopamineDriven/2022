{
  "name": "nest",
  "version": "1.0.0",
  "main": "src/main.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "introspect:rover": "rover graph introspect src/schema.gql",
    "update:schema": "ts-node ./src/graphql.schema.ts",
    "generate:secret": "openssl rand -hex 32",
    "codegen": "graphql-codegen --config codegen.yml",
    "seed": "ts-node --transpile-only ./seeds/seed.ts",
    "clear": "ts-node --transpile-only ./seeds/clear.ts",
    "redis:server:start:globally": "sudo systemctl enable redis-server",
    "docker:process:status": "docker ps",
    "docker:postgres:redis:build": "docker compose -f docker-compose.pgredis.yml up --build -d",
    "docker:postgres:redis:up": "docker compose -f docker-compose.pgredis.yml up -d",
    "docker:postgres:redis:down": "docker compose -f docker-compose.pgredis.yml down",
    "docker:pg:up": "docker compose -f docker-compose.pg.yml up -d",
    "docker:pg:down": "docker compose -f docker-compose.pg.yml down",
    "dev": "nodemon --config nodemon.json  src/main.ts",
    "dev:debug": "nodemon --config nodemon.json --inspect-brk -r tsconfig-paths/register -r src/main.ts",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "postbuild": "prisma generate",
    "start:nest": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nodemon --config nodemon-debug.json",
    "start:prod": "node dist/src/main.js",
    "start": "ts-node --transpile-only ./src/main.ts",
    "register:paths": "ts-node tsconfig-paths/register -r",
    "prisma:generate": "prisma generate",
    "prisma:generate:watch": "yarn prisma generate --watch",
    "prisma:studio": "prisma studio --port 3050",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:dev": "prisma migrate dev --preview-feature",
    "prisma:migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "prisma:migrate:reset": "prisma migrate reset --preview-feature",
    "prisma:migrate:deploy": "yarn prisma migrate deploy --preview-feature",
    "prisma:migrate:status": "yarn prisma migrate status --preview-feature",
    "prisma:migrate:resolve": "yarn prisma migrate resolve --preview-feature",
    "docker:build": "docker build -t nest-api .",
    "docker:dev": "docker build -f Dockerfile.dev -t nest-api .",
    "docker:dev:run": "docker run -p 80:3000 -f Docker.dev --env-file .env -d nest-api",
    "docker:run": "docker run -p 3000:3000 --env-file .env nest-api",
    "docker:pgredis:up": "docker compose -f docker-compose.with-redis.yml up -d",
    "docker:pgredis:down": "docker compose -f docker-compose.with-redis.yml down",
    "postgres:dev:up": "docker compose -f docker-compose.dev.yml up -d",
    "postgres:dev:down": "docker compose -f docker-compose.dev.yml down",
    "start:db": "pg_ctl -D /var/lib/postgresql/data -l logfile star",
    "start:psql": "docker compose run db bash",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\" \"graphql/**/*.graphql\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
